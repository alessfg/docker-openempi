<file-loader-map
	xsi:schemaLocation="http://configuration.openempi.openhie.org/fileloadermap fileloadermap.xsd"
	xmlns="http://configuration.openempi.openhie.org/fileloadermap"
	xmlns:fl="http://configuration.openempi.openhie.org/fileloadermap"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	delimiter=";"
	header-first-line="false">
	<fields>
		<field
			datatype="String">
			<column-index>1</column-index>
			<field-name>district</field-name>
		</field>
		<field
			datatype="String">
			<column-index>2</column-index>
			<field-name>districtId</field-name>
		</field>
		<field
			datatype="String">
			<column-index>3</column-index>
			<field-name>sector</field-name>
		</field>
		<field
			datatype="String">
			<column-index>4</column-index>
			<field-name>sectorId</field-name>
		</field>
		<field
			datatype="String">
			<column-index>5</column-index>
			<field-name>cell</field-name>
		</field>
		<field
			datatype="String">
			<column-index>6</column-index>
			<field-name>cellId</field-name>
		</field>
		<field
			datatype="String">
			<column-index>7</column-index>		
			<field-name>village</field-name>
		</field>
		<field
			datatype="String">
			<column-index>8</column-index>
			<field-name>villageId</field-name>
		</field>
		<field
			is-ignored="true">
			<column-index>9</column-index>
			<field-name/>
		</field>
		<field
			datatype="String">
			<column-index>10</column-index>
			<field-name>familyName</field-name>
		</field>
		<field
			datatype="String">
			<column-index>11</column-index>
			<field-name>givenName</field-name>
		</field>
		<field
			datatype="String"
			is-identifier="true"
			identifier-domain-name="National Identifier"
			namespace-identifier="NID"
			universal-identifier="NID"
			universal-identifier-type-code="NID">
			<column-index>12</column-index>
			<field-name>identifier</field-name>
		</field>
		<field
			datatype="Date"
			date-format-string="yyyy">
			<column-index>13</column-index>
			<field-name>dateOfBirth</field-name>
		</field>
		<field
			datatype="String">
			<column-index>14</column-index>
			<field-name>gender</field-name>
		</field>
	</fields>
</file-loader-map>
