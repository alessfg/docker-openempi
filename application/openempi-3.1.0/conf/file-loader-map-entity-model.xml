<file-loader-map
	xsi:schemaLocation="http://configuration.openempi.openhie.org/fileloadermap fileloadermap.xsd"
	xmlns="http://configuration.openempi.openhie.org/fileloadermap"
	xmlns:fl="http://configuration.openempi.openhie.org/fileloadermap"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	delimiter=","
	header-first-line="true">
	<fields>
		<field
			datatype="String"
			one-to-many="true"
			is-identifier="true"
			delimiter=":">
			<subfields>
				<field
					datatype="String">
					<column-index>1</column-index>
					<field-name>identifier</field-name>
				</field>
				<field
					datatype="String">
					<column-index>2</column-index>
					<field-name>namespaceIdentifier</field-name>
				</field>
				<field
					datatype="String">
					<column-index>3</column-index>
					<field-name>universalIdentifier</field-name>
				</field>
				<field
					datatype="String">
					<column-index>4</column-index>
					<field-name>universalIdentifierTypeCode</field-name>
				</field>
			</subfields>
		</field>
		<field
			datatype="String"
			one-to-many="true"
			is-identifier="true"
			delimiter=":">
			<subfields>
				<field
					datatype="String">
					<column-index>1</column-index>
					<field-name>identifier</field-name>
				</field>
				<field
					datatype="String">
					<column-index>2</column-index>
					<field-name>namespaceIdentifier</field-name>
				</field>
				<field
					datatype="String">
					<column-index>3</column-index>
					<field-name>universalIdentifier</field-name>
				</field>
				<field
					datatype="String">
					<column-index>4</column-index>
					<field-name>universalIdentifierTypeCode</field-name>
				</field>
			</subfields>
		</field>
		<field
			datatype="String"
			one-to-many="true"
			is-identifier="true"
			delimiter=":">
			<subfields>
				<field
					datatype="String">
					<column-index>1</column-index>
					<field-name>identifier</field-name>
				</field>
				<field
					datatype="String">
					<column-index>2</column-index>
					<field-name>namespaceIdentifier</field-name>
				</field>
				<field
					datatype="String">
					<column-index>3</column-index>
					<field-name>universalIdentifier</field-name>
				</field>
				<field
					datatype="String">
					<column-index>4</column-index>
					<field-name>universalIdentifierTypeCode</field-name>
				</field>
			</subfields>
		</field>
		<field
			datatype="String"
			one-to-many="true"
			is-identifier="true"
			delimiter=":">
			<subfields>
				<field
					datatype="String">
					<column-index>1</column-index>
					<field-name>identifier</field-name>
				</field>
				<field
					datatype="String">
					<column-index>2</column-index>
					<field-name>namespaceIdentifier</field-name>
				</field>
				<field
					datatype="String">
					<column-index>3</column-index>
					<field-name>universalIdentifier</field-name>
				</field>
				<field
					datatype="String">
					<column-index>4</column-index>
					<field-name>universalIdentifierTypeCode</field-name>
				</field>
			</subfields>
		</field>
		<field
			datatype="String">
			<column-index>5</column-index>
			<field-name>familyName</field-name>
		</field>
		<field
			datatype="String">
			<column-index>6</column-index>
			<field-name>givenName</field-name>
		</field>
		<field
			datatype="Date"
			date-format-string="yyyyMMdd">
			<column-index>7</column-index>
			<field-name>dateOfBirth</field-name>
		</field>		
		<field
			datatype="String">
			<column-index>8</column-index>
			<field-name>gender</field-name>
		</field>		
		<field
			datatype="String">
			<column-index>9</column-index>
			<field-name>address1</field-name>
		</field>
		<field
			datatype="String">
			<column-index>10</column-index>
			<field-name>city</field-name>
		</field>		
		<field
			datatype="String">
			<column-index>11</column-index>
			<field-name>state</field-name>
		</field>
		<field
			datatype="String">
			<column-index>12</column-index>
			<field-name>postalCode</field-name>
		</field>
		<field
			datatype="String">
			<column-index>13</column-index>
			<field-name>country</field-name>
		</field>
		<field
			datatype="String">
			<column-index>14</column-index>
			<field-name>race</field-name>
		</field>	
	</fields>
</file-loader-map>

